<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder
6.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_0_0.dtd">
<service-builder package-path="com.alacrity.openingdesign.messageportlet.model">
<author>Henricor</author>
<namespace>SK</namespace>
<entity name="SKQuestion" local-service="true" remote-service="false">
<column name="Question_ID" type="long" primary="true" />
<column name="Parent_ID" type="long" />
<column name="Title" type="String" />
<column name="Url" type="String" />
<column name="Post_Date" type="long" />
<column name="User_ID" type="long" />
<column name="companyId" type="long" />
<column name="groupId" type="long" />
<order by="desc">
<order-column name="Post_Date" />
</order>
<finder name="All" return-type="Collection">
<finder-column name="Parent_ID" />
</finder>
<finder name="findByURL" return-type="Collection">
<finder-column name="Url" />
</finder>
</entity>
</service-builder>